<?xml version="1.0" encoding="utf-8"?>
<ScrollView

    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true"

    android:background="#009700">


    <AbsoluteLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#ffffffff"
 >
        <ImageView
            android:layout_width="110dp"
            android:layout_height="62dp"
            android:id="@+id/imageView"
            android:background="@drawable/logo"
            android:layout_x="0dp" />

        <ImageView
            android:layout_width="177dp"
            android:layout_height="132dp"
            android:id="@+id/imageView3"
            android:layout_x="0dp"
            android:layout_y="121dp"
            android:background="@drawable/nani"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Nadia El khadji "
            android:id="@+id/name"
            android:layout_x="1dp"
            android:layout_y="92dp"
            android:textColor="#ff6450ff" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Dt.P. Diététiste-Nutritionniste"
            android:id="@+id/profession"
            android:layout_x="1dp"
            android:layout_y="281dp"
            android:textColor="#ff6450ff" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="P:+1 438-862-1184 "
            android:clickable="true"
            android:id="@+id/phone"
            android:layout_x="0dp"
            android:layout_y="307dp"
            android:textColor="#ff6450ff" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Address: 6963 St Hubert, Montréal, QC H2S 2N1"
            android:clickable="true"

            android:id="@+id/address"
            android:layout_x="-1dp"
            android:layout_y="334dp"
            android:textColor="#ff6450ff" />

















    </AbsoluteLayout>
</ScrollView>